{
  "id": "redis-master-pod",
  "kind": "Pod",
  "apiVersion": "v1beta1",
  "desiredState": {
    "manifest": {
      "version": "v1beta1",
      "id": "redis-master",
      "containers": [{
        "name": "redis-master",
        "image": "dockerfile/redis",
        "ports": [{"containerPort": 6379}]
      }]
    }
  },
  "labels": {
    "name": "redis-master",
    "app": "redis"
  },
  "nodeSelector":{
      "disktype":"spinning"
  }
}
    
